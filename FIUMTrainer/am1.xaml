<Page x:Class="am1"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:FIUMTrainer"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800"
      Title="am1">
	<Grid Background="White">
		<Grid.RowDefinitions>
			<RowDefinition/>
			<RowDefinition/>
		</Grid.RowDefinitions>
		<Grid.ColumnDefinitions>
			<ColumnDefinition/>
			<ColumnDefinition/>
		</Grid.ColumnDefinitions>
		<Button x:Name="button" Content="Teórico" HorizontalAlignment="Center" Margin="145,176,10,10" VerticalAlignment="Center" Width="245" Background="White" Height="39" FontSize="20"/>
		<Button x:Name="button_Copy" Content="Prácticos" HorizontalAlignment="Center" Margin="145,10,10,176" VerticalAlignment="Center" Width="245" Background="White" Height="39" Grid.Row="1" FontSize="20"/>
		<Button x:Name="button_Copy1" Content="Evaluación" HorizontalAlignment="Center" Margin="10,176,145,10" VerticalAlignment="Center" Width="245" Background="White" Height="39" Grid.Column="1" FontSize="20"/>
		<Button x:Name="button_Copy2" Content="?" HorizontalAlignment="Center" Margin="10,10,145,176" VerticalAlignment="Center" Width="245" Background="White" Height="39" Grid.Column="1" Grid.Row="1" FontSize="20"/>

	</Grid>
</Page>
